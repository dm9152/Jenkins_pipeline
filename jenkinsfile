pipeline {

    agent any 

    matrix {

        axes {

            axis {
                name 'os'
                values 'linux', 'windows', 'macos'
            }

            axis {
                name 'browser'
                values 'chrome', 'firefox', 'safari'

            }

            stages {

                stage('Build') {
                    steps {
                        echo "Building on ${os} with ${browser}"    
                    }
                }

                stage('TEST') {
                    steps {
                        echo "Running tests on ${os} with ${browser}"
                    }
                }
            }
        }
    }

    
}